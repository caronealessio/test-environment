<mvc:View controllerName="testenvironment.controller.Home"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core">
	<Page title="{i18n>title}">
		<Button text="TEST"
			press="onTest" />
		<Panel>
			<SelectList items="{MenuItem>/}"
				itemPress="onMenuItemPress">
				<c:Item key="{MenuItem>router}"
					text="{MenuItem>title}" />
			</SelectList>
		</Panel>
	</Page>
</mvc:View>

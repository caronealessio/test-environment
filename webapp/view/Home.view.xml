<mvc:View controllerName="testenvironment.controller.Home"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core">
	<Page title="{i18n>title}">
		<Button text="TEST"
			press="onTest" />
		<Panel>
			<SelectList items="{Functionalities>/}"
				itemPress="onFunctionality">
				<c:Item key="{Functionalities>Route}"
					text="{Functionalities>Text}" />
			</SelectList>
		</Panel>
	</Page>
</mvc:View>

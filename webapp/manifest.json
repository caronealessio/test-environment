{
  "_version": "1.59.0",
  "sap.app": {
    "id": "testenvironment",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.17.0",
      "toolsId": "d3d7fd79-3885-4480-b2a6-dcc1c5a55cc3"
    }
  },
  "sap.ui": {
    "theme": "sap_fiori_3",
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.135.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "testenvironment.i18n.i18n"
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "testenvironment.view",
        "async": true
      },
      "routes": [
        {
          "name": "home",
          "pattern": ":?query:",
          "target": ["home"]
        },
        {
          "name": "menuList",
          "pattern": "menu",
          "target": ["menuList"]
        },
        {
          "name": "menu",
          "pattern": "menu/{id}",
          "target": ["menu"]
        },
        {
          "name": "usersList",
          "pattern": "users",
          "target": ["usersList"]
        },
        {
          "name": "usersForm",
          "pattern": "users/{id}",
          "target": ["usersForm"]
        },
        {
          "name": "StandardTableManagement",
          "pattern": "functionality.StandardTableManagement.Dashboard",
          "target": ["StandardTableManagement"]
        },
        {
          "name": "CustomTableManagement",
          "pattern": "functionality.CustomTableManagement.Dashboard",
          "target": ["CustomTableManagement"]
        }
      ],
      "targets": {
        "home": {
          "name": "Home"
        },
        "menuList": {
          "name": "menu.MenuList"
        },
        "menu": {
          "name": "menu.Menu"
        },
        "usersList": {
          "name": "users.UsersList"
        },
        "usersForm": {
          "name": "users.UsersForm"
        },
        "StandardTableManagement": {
          "id": "StandardTableManagement",
          "name": "functionality.StandardTableManagement.Dashboard"
        },
        "CustomTableManagement": {
          "id": "CustomTableManagement",
          "name": "functionality.CustomTableManagement.Dashboard"
        }
      }
    },
    "rootView": {
      "viewName": "testenvironment.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
